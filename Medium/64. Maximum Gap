/**
 * @param {number[]} nums
 * @return {number}
 */
var maximumGap = function(nums) { 
    nums= nums.sort((a,b)=>b-a)
    if (nums.length <2) return 0
    let maxGap = 0
    for (let i = 0; i < nums.length-1 ; i++ ) {
        maxGap = Math.max(nums[i] - nums[i+1], maxGap)

    }
    return maxGap
};
