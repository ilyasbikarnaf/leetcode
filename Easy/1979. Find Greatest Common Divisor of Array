//Python
class Solution:
    def findGCD(self, nums: List[int]) -> int:
        gcd = 0

        for i in range(1,min(nums)+1):
            if(max(nums) % i ==0 and min(nums) % i ==0):
                gcd = i
        return gcd
        

// Javascript
/**
 * @param {number[]} nums
 * @return {number}
 */
var findGCD = function(nums) {
    nums.sort((a,b) => a-b)
    let max = nums[nums.length - 1]
    let min = nums[0]

    function pgcd(max, min) {
        if (max%min === 0) {
            return min
        }
        return pgcd(min , max%min)
    }

    return pgcd(max, min)
    


};
