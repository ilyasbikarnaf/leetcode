/**
 * @param {number[]} nums
 * @param {number} k
 * @param {number} multiplier
 * @return {number[]}
 */
var getFinalState = function(nums, k, multiplier) {


    for (let i = 0 ; i < k ; i++) {
        let min = Math.min(...nums)
        let minIndex = nums.findIndex(num => min == num)
        nums[minIndex] = min * multiplier

    }

    return nums
    

    
};
